	    			do
	    				local abilitySubPanel = Label(index[0]:getBp());				
	    				local isUpgraded = index[0]._upgraded;
	    				local avaible = index[0]:checkAvaibility(player);    				
	    				local abilitySubPanel = index[1];
						if avaible == false then
							if isUpgraded then
								if abilitySubPanel:has(index[2]) == false then
									abilitySubPanel:add(index[2]);									
								end
								abilitySubPanel:remove(index[3]);
							else
								if abilitySubPanel:has(index[3]) == false then
									abilitySubPanel:add(index[3]);
								end
							end 
						else
							abilitySubPanel:remove(index[3]);					
						end
	    			end
	    		end
