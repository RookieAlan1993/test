					if Player_IsHuman(player) then 
						Player_SetUpgradeAvailability( player , BP_GetUpgradeBlueprint(doc.bp), ITEM_REMOVED);					
					end
										
					docPanel:setIndex(key);
					docPanel:setDoctrineNameLabelText(doc.doc_name);
					docPanel:setDocIcon(doc.doc_icon);
					docPanel:setDoctrineBreifText(doc.doc_description);
					docPanel:setDoctrineBp(doc.bp);					
					docPanel:setRaceIndex(Player_GetRaceIndex(player));
					docPanel:setDoctrineBanner(doc.doc_banner);
				end			
			end
    	end

    	if messageType == Doctrine_CHOOSE_CALLBACK then    
