  			Command_PlayerUpgrade( player, doc , true, false );  										
		end
    	if messageType == UPGRADE_CALLBACK then 
    		local abilityUpgrade = BP_GetUpgradeBlueprint(message);
    		local abCost = Player_GetUpgradeCost( player, BP_GetUpgradeBlueprint(message), RT_Command);
    		local player_CP_Points = Player_GetResource( player, RT_Command );
    		Player_SetResource(player, RT_Command, player_CP_Points-abCost );
    		Command_PlayerUpgrade(player, abilityUpgrade , true, false );    		
    	end 

    	if messageType == AVILITIES_AVAIBILITY_CHECK_CALLBACK then
